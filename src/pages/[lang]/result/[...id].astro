---
import Result from '@theme/Result.astro';
import Layout from '@theme/Layout.astro';
import { availableLanguageTags, sourceLanguageTag } from '@/paraglide/runtime';

export function getStaticPaths() {
    return availableLanguageTags
        .filter((lang) => lang !== sourceLanguageTag)
        .map((lang) => ({ params: { lang, id: 'id' } }));
}
---

<Layout>
    <Result />
</Layout>
