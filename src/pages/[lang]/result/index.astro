---
import Result from '@/themes/default/Result.astro';
import Layout from '@/themes/default/Layout.astro';
import { availableLanguageTags, sourceLanguageTag } from '@/paraglide/runtime';

export function getStaticPaths() {
    return availableLanguageTags
        .filter((lang) => lang !== sourceLanguageTag)
        .map((lang) => ({ params: { lang } }));
}
---

<Layout>
    <Result />
</Layout>
