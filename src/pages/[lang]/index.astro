---
import Welcome from '@theme/StartTest.astro';
import Layout from '@theme/Layout.astro';
import { availableLanguageTags, sourceLanguageTag } from '@/paraglide/runtime';
import Article from '@theme/Article.astro';

console.log(sourceLanguageTag);

export function getStaticPaths() {
    return availableLanguageTags
        .filter((lang) => lang !== sourceLanguageTag)
        .map((lang) => ({ params: { lang } }));
}

export const prerender = true;
---

<Layout>
    <Welcome />
    <Article />
</Layout>
